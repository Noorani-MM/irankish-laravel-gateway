{
    "name": "irankish/laravel-gateway",
    "description": "Laravel package for integrating IranKish payment gateway (درگاه پرداخت ایران کیش برای لاراول)",
    "keywords": [
        "laravel",
        "payment",
        "gateway",
        "iran",
        "irankish",
        "shaparak",
        "iran kish",
        "laravel-irankish",
        "laravel-payment",
        "payment-gateway"
    ],
    "homepage": "https://github.com/irankish/laravel-gateway",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Mahdi Noorani",
            "email": "matio.nora2000@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "IranKish\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IranKish\\IranKishServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
