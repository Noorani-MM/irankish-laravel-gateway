{
    "name": "noorani-mm/irankish-laravel-gateway",
    "description": "Laravel package for integrating IranKish payment gateway (درگاه پرداخت ایران کیش برای لاراول)",
    "keywords": [
        "laravel",
        "payment",
        "gateway",
        "iran",
        "irankish",
        "shaparak",
        "iran kish",
        "irankish-laravel-gateway",
        "laravel-irankish",
        "laravel-payment",
        "payment-gateway"
    ],
    "homepage": "https://github.com/noorani-mm/irankish-laravel-gateway",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Mahdi Noorani",
            "email": "matio.nora2000@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0 || ^11.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "IranKish\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IranKish\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IranKish\\IranKishServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "issues": "https://github.com/noorani-mm/irankish-laravel-gateway/issues",
        "source": "https://github.com/noorani-mm/irankish-laravel-gateway"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
